{
    "printWidth": 100,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "vueIndentScriptAndStyle": true,
    "quoteProps": "as-needed",
    "htmlWhitespaceSensitivity": "css",
    "embeddedLanguageFormatting": "auto",
    "requirePragma": false,
    "insertPragma": false,
    "tabWidth": 4,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "tabWidth": 2,
                "useTabs": false
            }
        }
    ],
    "editorconfig": true
}
